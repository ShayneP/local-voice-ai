# Telnyx Provider Configuration
# Set these in your .env.local to use Telnyx for STT, LLM, and TTS

# Credentials
TELNYX_API_KEY=your_telnyx_api_key

# Speech-to-Text (STT)
TELNYX_STT_URL=https://ai.telnyx.com/v1/speech-to-text
TELNYX_STT_MODEL=whisper

# Language Model (LLM)
TELNYX_LLM_URL=https://ai.telnyx.com/v1/chat/completions
TELNYX_LLM_MODEL=qwen-telnyx

# Text-to-Speech (TTS)
TELNYX_TTS_URL=https://ai.telnyx.com/v1/text-to-speech
TELNYX_TTS_MODEL=kokoro
TELNYX_TTS_VOICE=af_nova